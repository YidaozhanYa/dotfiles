[Trigger]
Operation = Install
Operation = Upgrade
Type = Package
Target = wps-office-cn
[Action]
When = PostTransaction
Exec = /usr/bin/bash -c 'mv /usr/bin/wps /usr/bin/wps.bak; mv /usr/bin/wpp /usr/bin/wpp.bak; mv /usr/bin/et /usr/bin/et.bak; mv /usr/bin/wpspdf /usr/bin/wpspdf.bak; echo "" | base64 -d > /usr/bin/wps; echo "" | base64 -d > /usr/bin/wpp; echo "" | base64 -d > /usr/bin/et; echo "" | base64 -d > /usr/bin/wpspdf; chmod +x /usr/bin/wps; chmod +x /usr/bin/wpp; chmod +x /usr/bin/et; chmod +x /usr/bin/wpspdf'
